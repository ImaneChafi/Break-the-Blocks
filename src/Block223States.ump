class Game {
	gameStatus {
		notInLoop {
			playGame() -> inLoop;
			
		
		}
		inLoop {
			pauseGame() --> notInLoop;
			nextLevel(Game game, Level level) [!almostFinishedGame()] / {
				int levelIndex = level.indexOfLevel();
				levelIndex++;
				
			} -> notInLoop;
			nextLevel(Game game, Level level) [almostFinishedGame()] / {
				
			} -> notInLoop;
		
		}
	}
	
	private boolean almostFinishedGame() {
	}
}